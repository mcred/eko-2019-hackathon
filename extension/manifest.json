{
  "name": "Resume Masking",
  "version": "1.0",
  "description": "Resume masking",
  "permissions": ["activeTab"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "matches": ["*://cdn.lever.co/*"],
      "js": ["jquery-3.3.1.min.js", "content.js"],
      "css": ["css/altered.css"],
      "run_at": "document_idle"
    }
  ],
  "manifest_version": 2
}
